{
  "expo": {
    "name": "Elevate",
    "slug": "elevate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "Elevate-bit68",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "url": "https://u.expo.dev/2dd50375-6a01-4fc2-9878-e5f4f50aa0c0"
    },
    "assetBundlePatterns": ["**/*"],
